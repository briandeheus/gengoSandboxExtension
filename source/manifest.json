{
	"manifest_version": 2,
	"name": "Gengo Sandbox Extension",
	"description": "Unofficial Gengo Extension for Sandbox",
	"version": "1.0.0",
	"icons": {
		"16": "16x16.png",
		"48": "48x48.png",
		"128": "128x128.png"
	},
	"permissions": [
	  
		"http://sandbox.gengo.com/"
		
	],
  
	"content_scripts": [
	{
		"matches": ["http://sandbox.gengo.com/dashboard/"],
		"js": ["jquery.js", "async.js", "main.js"],
		"css": ["styles.css"],
		"run_at": "document_end"

	}
	
  ]
  
}